includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src
        - database
        - tests/TestModels/DummyModel.php
    reportUnmatchedIgnoredErrors: false
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    bootstrapFiles:
        - vendor/autoload.php
    ignoreErrors:
        - '#Call to an undefined static method .+\\Facades\\.+#'
        - '#Call to an undefined method .+getUploadableAttributes.+#'
        - '#Call to an undefined method .+getUploadPath.+#'
        - '#Access to an undefined property .+\\Model.+#'
        - '#Call to an undefined method christopheraseidl\\HasUploads\\Jobs\\Contracts\\Builder.+#'
        - '#Call to function method_exists\(\) with .Illuminate.+Support.+Str.+ and .pascal. will always evaluate to true\.#'